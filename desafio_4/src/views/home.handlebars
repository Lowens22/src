<div class="product-list" id="productsFeed">
    {{#each product}}
        <div class="product">
            <p class="title">{{title}}</p>
            <p class="description">{{description}}</p>
            <p class="price">{{price}}</p>
            <p class="thumbnail">{{thumbnail}}</p>
            <p class="code">{{code}}</p>
            <p class="stock">{{stock}}</p>
        </div>
    {{/each}}
</div>

